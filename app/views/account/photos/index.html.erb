<h1>Listing account_photos</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Description</th>
    <th>User</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @account_photos.each do |account_photo| %>
  <tr>
    <td><%= account_photo.title %></td>
    <td><%= account_photo.description %></td>
    <td><%= account_photo.user %></td>
    <td><%= link_to 'Show', account_photo %></td>
    <td><%= link_to 'Edit', edit_account_photo_path(account_photo) %></td>
    <td><%= link_to 'Destroy', account_photo, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Photo', new_account_photo_path %>
